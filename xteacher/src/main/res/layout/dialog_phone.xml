<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/input_dialog_board"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    

    <RelativeLayout
        android:background="@drawable/yuanjiao"
        android:layout_height="@dimen/space_135"
        android:layout_width="@dimen/space_251"
        android:layout_centerInParent="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/space_28"
            android:textColor="@color/text_color32"
            android:textSize="@dimen/sp27"
            android:text="400-600-0000"/>

        <View
            android:layout_width="match_parent"
            android:layout_above="@+id/dialog_phone_cancle"
            android:layout_height="@dimen/dp_0.5"
            android:background="@color/activity_personal_calendar_detail_view"/>
        <TextView
            android:id="@+id/dialog_phone_ok"
            android:layout_alignParentBottom="true"
            android:layout_width="@dimen/dp_125"
            android:layout_height="@dimen/dp_53"
            android:textColor="@color/dialog_phone_call"
            android:textSize="@dimen/space_19"
            android:gravity="center"
            android:text="@string/call" />

        <View
            android:layout_width="@dimen/dp_0.5"
            android:layout_height="@dimen/dp_53"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            android:background="@color/activity_personal_calendar_detail_view"/>

        <TextView
            android:layout_alignParentBottom="true"
            android:id="@+id/dialog_phone_cancle"
            android:layout_alignParentRight="true"
            android:layout_width="@dimen/dp_125"
            android:layout_height="@dimen/dp_53"
            android:gravity="center"
            android:textColor="@color/dialog_phone_cancel"
            android:textSize="@dimen/space_19"
            android:text="@string/pickerview_cancel" />
    </RelativeLayout>

    
</RelativeLayout>
